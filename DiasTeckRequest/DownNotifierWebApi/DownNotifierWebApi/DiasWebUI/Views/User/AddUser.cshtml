
@{
    ViewData["Title"] = "AddUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Kullanıcı Ekle</h1>
<form >
    <div class="form-group">
        @Html.Label("Kullanıcı Adı")
        <input type="text" placeholder=" Kullanıcı Adı" id="kullanici_adi" class="form-control" />
        <br /> <br />
        @Html.Label("Kullanıcı Şifre")
        <input type="text" placeholder="Kullanıcı Şifre" id="kullanici_sifre" class="form-control" />
        <br /><br />
        <button  id="kaydet" class="btn btn-info">Kaydet</button>
    </div>
</form>
@section Scripts{
    <script src="~/pageJs/User/addUser.js"></script>
}

